                  .filter__categories
                    p.filter__title Категория
                    label.filter__label.label-checkbox(for="sofa")
                      input.filter__checkbox.checkbox(id="sofa" type="checkbox" checked)
                      span.filter__text.filter__text-checkbox Диваны
                    label.filter__label.label-checkbox(for="armchair")
                      input.filter__checkbox.checkbox(id="armchair" type="checkbox")
                      span.filter__text.filter__text-checkbox Кресла
                .filter__price
                  p.filter__title.filter__title-price Цена
                  .filter__range
                    label.filter__label(for="from")
                      span.filter__text.filter__text-from от
                      input.filter__input(id="from" type="number" value="2000" min="2000" max="150000")
                    label.filter__label(for="to")
                      span.filter__text.filter__text-to до
                      input.filter__input(id="to" type="number" value="150000" min="2000" max="150000")
