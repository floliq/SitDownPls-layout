    main.content
      .container
        .content__content
          .content__parts
            section.filter
              h2.subheading.filter__heading.filter__heading-md Каталог
              h2.subheading.filter__heading Фильтровать по:
              .filter__content
                .filter__properties
                  .filter__colors
                    p.filter__title Цвет
                    label.filter__label.label-checkbox(for="brown")
                      input.filter__checkbox.checkbox(id="brown" type="checkbox")
                      span.filter__text.filter__text-checkbox Коричневый
                    label.filter__label.label-checkbox(for="black")
